<table class="table">
    <thead>
    <tr>
        <% for (let property in queryResults[0]) { %>
            <th>
                <%= property %>
            </th>
        <% } %>
    </tr>
    </thead>
    <tbody>
    <% for (let i = 0; i < queryResults.length; i++) {
        let queryItem = queryResults[i]
    %>
    <tr>
        <% for (let property in queryItem) { %>
            <td>
                <%= queryItem[property] %>
            </td>
        <% } %>
    </tr>
    <% } %>
    </tbody>
</table>